<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <root level="info">
        <appender-ref ref="STDOUT"/>
    </root>
    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%d{yyyy/MM/dd HH:mm:ss} %-5level [%thread] - %msg%n</pattern>
        </encoder>
    </appender>
</configuration>
